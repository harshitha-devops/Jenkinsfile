pipeline {
    agent any

    stages {
        stage('clone code from git') {
            steps {
                echo 'clone code success'
            }
        }
         stage('build code') {
            steps {
                echo 'code bulid success'
            }
        }
         stage('code package') {
            steps {
                echo 'code packaged'
            }
        }
         stage('Test code') {
            steps {
                echo 'Testing Success'
            }
        }
         stage('deploy code') {
            steps {
                echo 'code deployed'
            }
        }
    }
}
